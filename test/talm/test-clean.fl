const t0, 10
const t1, 4

// Chamada para addwrap(10, 4)
callgroup('cg1', 'addwrap')
callsnd snd1, t0, cg1
callsnd snd2, t1, cg1
retsnd rtsnd0, addwrap[0], cg1

// Recebe resultado em variável result
addi result, rtsnd0, 0

ret main, result, [cg1]

// Função wrapper: addwrap(x, y) = x + y
addwrap:
